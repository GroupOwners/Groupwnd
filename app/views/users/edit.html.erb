<h1>Update your profile</h1>
<%= form_for @user do |form| %>

  <% if @user.errors.any? %>
    <%= pluralize(@user.errors.count, "error") %> prevented your account from being changed:
    <ul>
      <% @user.errors.full_messages.each do |error_message| %>
        <li><%= error_message %></li>
      <% end %>
    </ul>
  <% end %>

  <fieldset class="email">
    <%= form.label :email %>
    <%= form.email_field :email %>
  </fieldset>



 

  
 
  <fieldset class="update">
    <%= form.submit "Update" %>
  </fieldset>
<% end %>
