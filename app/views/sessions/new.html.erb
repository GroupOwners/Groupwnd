<%= form_for :session, url: session_path do |form| %>
  <fieldset class="email">
    <%= form.label :email %>
    <%= form.email_field :email %>
  </fieldset>
  <fieldset class="password">
    <%= form.label :password %>
    <%= form.password_field :password %>
  </fieldset>
  <fieldset class="sign-up">
    <%= form.submit "Log In" %>
  </fieldset>
<% end %>

<%= link_to "Sign Up", new_user_path %>
